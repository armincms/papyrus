{
    "name": "armincms/papyrus",
    "description": "Foundation of the `Armincms` page manager.",
    "keywords": [
        "armincms"
    ],
    "license": "proprietary",
    "require": {
        "php": ">=7.1.0",
        "armincms/markable": "^1.0",
        "armincms/nova-targomaan": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Armincms\\Papyrus\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Armincms\\Papyrus\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true, 
    "version": "2.0.0"
}
